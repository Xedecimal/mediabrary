<div id="tv-series">
<callback ds="_d" name="tv.cb.buttons" />

<input type="hidden" id="detail-type" value="tv-series" />
<input type="hidden" id="detail-id" value="{{Data._id}}" />
<input type="hidden" id="detail-path" value="{{Path}}" />

<div id="details">
<table class="table-list">
<tr><th>Index</th><th>Date</th><th>Title</th></tr>
<item>
	<tr class="tv-episode" title="{{path}}" id="item:{{Data._id}}">
		<td>S{{season}}E{{episode}}</td>
		<td>{{details.TVDB.FirstAired}}</td>
		<td><a class="episode-link"
			href="player?path={{url}}">{{title}}</a></td>
	</tr>
</item>
</table>
</div> <!-- #tv-details -->
</div> <!-- #tv-series -->