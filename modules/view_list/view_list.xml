<div id="view-list">

<link type="text/css" rel="stylesheet" href="view_list/view_list.css" />
<script type="text/javascript" src="view_list/view_list.js"></script>

<table><tr>
	<th><input type="text" value="Name" /> &uarr; &darr;</th>
	<th><input type="text" value="Date" size="5" /> &uarr; &darr;</th>
	<th><input type="text" value="Parent" /> &uarr; &darr;</th>
	<th><input type="text" value="Sibling Index" /> &uarr; &darr;</th>
</tr>

<item>
	<tr class="tr-list-item {{class}}">
		<td><a href="{{type}}/detail/{{_id}}" class="a-title">{{title}}</a></td>
		<td>{{released}}</td>
		<td>{{parent}}</td>
		<td>{{index}}</td>
	</tr>
</item>

</table>
</div> <!-- #view-list -->